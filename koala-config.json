{
    "mappings": [{
        "src": "assets/source",
        "dest": "assets/pub"
    }],

    "ignores": ["*.min.js", "*.min.css", "*.lib.css", "assets/source/css/bootstrap", "assets/source/css/libs", "assets/source/js/libs", "assets/pub", "vendor/", "_*"],

    "options": {
        "bare": false,
        "literate": false,
        "outputStyle": "compress",
        "lineComments": true,
        "debugInfo": false,
        "strictMath": false,
        "strictUnits": false,
        "sourceMap": false
    },

    "customOptions": []
}